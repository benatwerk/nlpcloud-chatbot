{
    "name": "nlpcloud-chat",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "npm run dev",
        "dev": "npm-run-all --parallel server vite",
        "dev:host": "npm-run-all --parallel server \"vite -- --host\"",
        "vite": "vite",
        "server": "node server.js",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "axios": "^1.5.1",
        "classnames": "^2.3.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsdom": "^22.1.0",
        "nlpcloud": "^1.0.48",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
        "sqlite3": "^5.1.6",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.0.0",
        "@types/chai": "^4.3.8",
        "@types/jsdom": "^21.1.3",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "chai": "^4.3.10",
        "eslint": "^8.45.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "npm-run-all": "^4.1.5",
        "sass": "^1.68.0",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vitest": "^0.34.6"
    }
}
